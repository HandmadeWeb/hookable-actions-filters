{
    "name": "michaelr0/hookable-actions-filters",
    "description": "",
    "keywords": [
        "michaelr0",
        "hookable",
        "actions",
        "filters"
    ],
    "homepage": "https://github.com/michaelr0/hookable-actions-filters",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Michael Rook",
            "email": "michael@rook.net.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "illuminate/collections": "^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Michaelr0\\HookableActionsAndFilters\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Michaelr0\\HookableActionsAndFilters\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Michaelr0\\HookableActionsAndFilters\\Providers\\HookableActionsAndFiltersServiceProvider"
            ],
            "aliases": {
                "Action": "Michaelr0\\HookableActionsAndFilters\\Action",
                "Filter": "Michaelr0\\HookableActionsAndFilters\\Filter"
            }
        }
    }
}
